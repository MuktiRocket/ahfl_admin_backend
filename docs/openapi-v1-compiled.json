    "/auth/verify-reset-code": {
      "post": {
        "tags": [
          "Admin Authentication"
        ],
        "summary": "Verify your self using an authentication code sent in email",
        "security": [
          {
            "BearerGuestIdTokenAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "email",
                  "resetCode"
                ],
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email",
                    "minLength": 6,
                    "maxLength": 30
                  },
                  "resetCode": {
                    "type": "string",
                    "pattern": "^[0-9]+$",
                    "minLength": 6,
                    "maxLength": 6
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Returns a valid token on successful validation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": [
                    "token"
                  ],
                  "properties": {
                    "token": {
                      "type": "string",
                      "description": "JWT token for authentication"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized - Invalid credentials provided.",
            "$ref": "#/components/responses/unauthorized"
          },
          "403": {
            "description": "Forbidden - User is not authorized to access this resource.",
            "$ref": "#/components/responses/forbidden"
          },
          "500": {
            "description": "Internal Server Error - An unexpected error occurred.",
            "$ref": "#/components/responses/internalServerError"
          }
        }
      }
    },
    "/auth/forget-password": {
      "post": {
        "tags": [
          "Admin Authentication"
        ],
        "summary": "Attempt to reset the password if forget",
